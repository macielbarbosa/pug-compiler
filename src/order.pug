
mixin order(value)
  .order
    table
      tr.row-header
        th Pedido
        th SKU
        th CD/CNPJ
        th OC
        th Quantidade
        th Preço unitário (+IPI)
        th Previsão de faturamento
        th Comissão
      tr.row-data
        td Nº #{value.id}
        td.sku
          if value.internacional
            img(src='data:image/png;base64,')
          span #{value.sku}
        td 
          span #{value.cd}
          br
          span.cnpj #{value.cnpj}
        td Nº #{value.oc}
        td #{value.quantidade}
        td #{value.precoUnitario.toLocaleString('pt-BR', { style: 'currency', currency: 'BRL' })}
        td #{value.previsaoFaturamento}
        td #{value.comissao}%


    